  61          //========================================================================
  62          // 函数: u8 Ext_Inilize(u8 EXT, EXTI_InitTypeDef *INTx)
  63          // 描述: 外部中断初始化程序.
  64          // 参数: INTx: 结构参数,请参考Exti.h里的定义.
  65          // 返回: 成功返回0, 空操作返回1,错误返回2.
  66          // 版本: V1.0, 2012-10-22
  67          //========================================================================
  68          u8  Ext_Inilize(u8 EXT, EXTI_InitTypeDef *INTx)
  69          {
  70   1        if(EXT >  EXT_INT4) return 1; //空操作
  71   1        
  72   1        if(EXT == EXT_INT0) //外中断0
  73   1        {
  74   2          if(INTx->EXTI_Interrupt == ENABLE)    EX0 = 1;  //允许中断
  75   2          else                  EX0 = 0;  //禁止中断
  76   2          if(INTx->EXTI_Polity == PolityHigh)   PX0 = 1;  //高优先级中断
  77   2          else                  PX0 = 0;  //低优先级中断
  78   2          if(INTx->EXTI_Mode == EXT_MODE_Fall)  IT0 = 1;  //下降沿中断
  79   2          else                  IT0 = 0;  //上升,下降沿中断
  80   2          return  0;    //成功
  81   2        }
  82   1      
  83   1        if(EXT == EXT_INT1) //外中断1
  84   1        {
  85   2          if(INTx->EXTI_Interrupt == ENABLE)    EX1 = 1;  //允许中断
  86   2          else                  EX1 = 0;  //禁止中断
  87   2          if(INTx->EXTI_Polity == PolityHigh)   PX1 = 1;  //高优先级中断
  88   2          else                  PX1 = 0;  //低优先级中断
  89   2          if(INTx->EXTI_Mode == EXT_MODE_Fall)  IT1 = 1;  //下降沿中断
  90   2          else                  IT1 = 0;  //上升,下降沿中断
  91   2          return  0;    //成功
  92   2        }
  93   1      
  94   1        if(EXT == EXT_INT2)   //外中断2, 固定为下降沿低优先级中断
  95   1        {
  96   2          if(INTx->EXTI_Interrupt == ENABLE)  INT_CLKO |=  (1 << 4);  //允许中断  
  97   2          else                INT_CLKO &= ~(1 << 4);  //禁止中断
  98   2          return  0;    //成功
  99   2        }
 100   1      
 101   1        if(EXT == EXT_INT3)   //外中断3, 固定为下降沿低优先级中断
 102   1        {
 103   2          if(INTx->EXTI_Interrupt == ENABLE)  INT_CLKO |=  (1 << 5);  //允许中断  
 104   2          else                INT_CLKO &= ~(1 << 5);  //禁止中断
 105   2          return  0;    //成功
 106   2        }
 107   1      
 108   1        if(EXT == EXT_INT4)   //外中断4, 固定为下降沿低优先级中断
 109   1        {
 110   2          if(INTx->EXTI_Interrupt == ENABLE)  INT_CLKO |=  (1 << 6);  //允许中断  
 111   2          else                INT_CLKO &= ~(1 << 6);  //禁止中断
 112   2          return  0;    //成功
 113   2        }
 114   1        return 2; //失败
 115   1      }
